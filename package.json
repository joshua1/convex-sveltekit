{
  "name": "convex-sveltekit",
  "version": "0.1.0",
  "description": "SvelteKit-native Convex integration. Real-time queries, form spreading, SSR-to-live transport.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "@sveltejs/kit": "^2.51.0",
    "convex": "^1.17.0",
    "svelte": "^5.29.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^6.3.1",
    "@sveltejs/kit": "^2.51.0",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "convex": "^1.31.7",
    "svelte": "^5.50.3",
    "svelte-check": "^4.3.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "zod": "^4.3.6"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "convex",
    "real-time",
    "ssr",
    "forms",
    "transport"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/axel-rock/convex-sveltekit"
  },
  "homepage": "https://github.com/axel-rock/convex-sveltekit#readme",
  "author": "Axel Rock"
}
